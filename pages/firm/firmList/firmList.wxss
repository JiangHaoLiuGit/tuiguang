/* pages/firm/firmList/firmList.wxss */
.head{
	background: url("data:image/png;base64,") no-repeat;
	background-color: #2E95FA;
  padding: 0 28rpx 18rpx 30rpx;
  width: 750rpx;
  height: 222rpx;
  overflow: hidden;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 12;
}
.head image{
  width: 20rpx;
  height: 35rpx;
}
.head text{
  font-size: 36rpx;
  line-height: 48rpx;
  text-align: center;
  display: block;
  color: #fff;
}
.head .headTop{
	position: unset;
	background-color: transparent;
	padding-left: 0;
	padding-right: 0;
}
.head .tab{
	margin-top: 10rpx;
  display:flex;
  justify-content: space-between;
  align-items: center;
}
.head .tab .cont{
  display: initial;
  height: 42rpx;
  font-size: 32rpx;
  line-height: 42rpx;
  border-bottom: 4rpx solid #2E95FA;
  padding-bottom: 49rpx;
	color: #AFD7FF;
	z-index: 10;
}
.head .tab .cont.active{
  border-color: #FDFDFD;
  color: #fff;
}
.tabView{
	margin-top: 222rpx;
	padding: 54rpx 30rpx 78rpx 30rpx;
	min-height: calc(100vh - 222rpx);
}
.tabView .cont{
	position: relative;
	margin-top: 40rpx;
	padding: 20rpx 38rpx 24rpx 18rpx;
	box-shadow: 0 6px 12px rgba(0,0,0,.08);
	width: 100%;
}

.tabView .cont:first-child{
	margin-top: 0rpx;
}
.tabView .cont.failCont{
	padding: 0;
}
.tabView .cont.failCont .failView{
	padding: 14rpx 38rpx 14rpx 18rpx;
	height: 130rpx;
	transition: left 0.1s linear;
	position: absolute;
	top: 0;
	z-index: 10;
	background: #fff;
	width:100%;
}
.viewC{
	width: 100%;
	height: 80rpx;
	background-color: #000;
	color: #fff;
}
.shan{
	width: 65rpx;
	height: 80rpx;
	background-color: red;
	color: #fff;
}
.cccc{
	height: 500rpx;
	display: block;
	background-color: #ccc;
}
.tabView .contFail>.failViews{
	color: #9a9a9a;
	text-align: center;
}
.tabView .cont.failCont .bottonView{
	width: 100%;
	height: 130rpx;
}
.tabView .cont.fail .failView
.tabView .cont:first-child{
	margin-top: 30rpx;
}
.tabView .cont .firmImg{
	font-family: 'PingFang SC';
	font-weight: bold;
	font-size: 52rpx;
	line-height: 28rpx;
	width: 74rpx;
	height: 74rpx;
	border-radius: 50%;
	border:2rpx #2E95FA solid;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	margin-top: 12rpx;
}
.tabView .cont .firmImg image{
	width: 52rpx;
}
.tabView .cont .firmInfo{
	padding-left: 18rpx;
	width: 545rpx;
}
.tabView .cont .firmInfo .firmName{
	position: relative;
	padding-bottom: 10rpx;
	border-bottom:2rpx solid #F2F2F2;
}
.tabView .cont .firmInfo .firmName .name{
	font-size: 32rpx;
	line-height: 42rpx;
	color: #333;
	width: 450rpx;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
}
.tabView .cont .firmInfo .firmName .staus{
	position: absolute;
	right: -38rpx;
	top: 0;
	width: 86rpx;
	height: 40rpx;
	border-top-left-radius:20rpx;
	border-bottom-left-radius:20rpx;
	text-align: center;
	line-height: 40rpx;
	font-size: 24rpx;
	display: block;
	color: #fff;
}
.tabView .cont .firmInfo .firmPro{
	color: #666;
	font-size: 24rpx;
	line-height: 32rpx;
}
.tabView .cont .bottonView{
	
}
.tabView .cont .firmSrc{
	width: 130rpx;
	height: 130rpx;
	background: #666;
	box-shadow: 0 6px 12px rgba(0,0,0,.08);
	/* right: -130rpx; */
	padding: 32rpx 24rpx 24rpx;
	text-align: center;
	border-top-left-radius: 0;
	border-bottom-left-radius: 0;
	border-top-right-radius: 8rpx;
	border-bottom-right-radius: 8rpx;
	line-height: 41rpx;
}
.tabView .cont .firmSrc image{
	width: 40rpx;
	height: 40rpx;
	margin-bottom: 6rpx;
	display: inline-block;
}
.tabView .cont .firmSrc text{
	font-size: 20rpx;
	color: #fff;
	display: block;
	position: relative;
	bottom: 15rpx;
}
.tabView .cont .firmFail{
	background: #E8E8E8;
	color: #FC5E4B;
	font-size: 24rpx;
	word-wrap:break-word;
	padding: 16rpx 20rpx;
}
.tabView .cont .firmFail .text{
	display: block;
	line-height: 34rpx;
	width: 100%;
}

.not{
	background: #2E95FA;
}
.success{
	background: #2AC980;
}
.fail{
	background: #FC5E4B;
}
.yellow{
	background: #FEAA3C;
}
.notB{
	border-color: #2E95FA!important;
	color: #2E95FA!important;
}
.successB{
	border-color: #2AC980!important;
	color: #2AC980!important;
}
.failB{
	border-color: #FC5E4B!important;
	color: #FC5E4B!important;
}