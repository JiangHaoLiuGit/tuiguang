
.margin{
	margin-left: 30rpx;
	margin-right: 30rpx;
}
.top{
	box-sizing: border-box;
	background: url("data:image/png;base64,");
	padding: 86rpx 0 130rpx 54rpx;
	background-color:#2E95FA ;
	background-repeat: no-repeat;
	background-size: contain;
	position: relative;
	overflow: hidden;
}

.top .userImg{
	width: 120rpx;
	height: 120rpx;
	margin-right: 46rpx;
	border-radius: 50%;
	z-index: 2;
	border-radius: 50%;
  overflow: hidden;
}
.top .userImg image{
	width: 120rpx;
	height: 120rpx;
	
}
.top .userInfo{
	z-index: 2;
}
.top .userInfo .userName{
	font-size: 32rpx;
	line-height: 42rpx;
	color: #fff;
	margin-bottom: 9rpx;
}
.top .userInfo .userTime{
	font-size: 24rpx;
	line-height: 32rpx;
	color: #fff;
}
.not{
	background: #2E95FA;
}
.success{
	background: #2AC980;
}
.fail{
	background: #FC5E4B;
}
.notB{
	border-color: #2E95FA!important;
	color: #2E95FA!important;
}
.successB{
	border-color: #2AC980!important;
	color: #2AC980!important;
}
.failB{
	border-color: #FC5E4B!important;
	color: #FC5E4B!important;
}
.yellow{
	background: #FEAA3C;
}
.center{
	min-height: 101rpx;
	padding: 24rpx 20rpx 16rpx 20rpx;
	background: #fff;
	display:flex;
	justify-content: space-between;
	box-shadow:0 3px 6px rgba(0,0,0,.16);
	position: relative;
	bottom: 60rpx;
}
.center .content{
	
}
.center .content .block{
	width: 110rpx;
	height: 110rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	border-radius: 8rpx;
}
.center .content .block image{
	width: 72rpx;
}
.center .content p{
	margin-top: 14rpx;
	font-size: 28rpx;
	line-height: 38rpx;
}
.bottom{
	margin-top: 16rpx;
}
.bottom>p.title{
	font-size: 36rpx;
	line-height: 48rpx;
	color: #333;
	font-weight: bold;
}
.bottom .box{
	height: calc(100vh - (48rpx + 16rpx + 202rpx + 352rpx + 50rpx + 30rpx));
	margin-top: 30rpx;
	padding: 0 30rpx 0;
	overflow-y: scroll;
}
.bottom .point .box{
	height: 77vh;
}
.bottom .box .block>text{
	color: #9a9a9a;
    display: block;
    width: 100%;
    /* text-align: center; */
    margin-top: 20rpx;
}
.bottom .box .block>.cont{
	margin-top: 40rpx;
	padding: 20rpx 38rpx 24rpx 18rpx;
	box-shadow: 0 6px 12px rgba(0,0,0,.08);
}
.bottom .box .block>.cont:first-child{
	margin-top: 0;
}
.bottom .box .block>.cont .firmImg{
	font-family: 'PingFang SC';
	font-weight: bold;
	font-size: 52rpx;
	line-height: 28rpx;
	width: 74rpx;
	height: 74rpx;
	border-radius: 50%;
	border:2rpx solid;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	margin-top: 12rpx;
}
.bottom .box .block>.cont .firmImg .font{
}
.bottom .box .block>.cont .firmImg image{
	width: 52rpx;
}
.bottom .box .block>.cont .firmInfo{
	padding-left: 18rpx;
	width: 545rpx;
}
.bottom .box .block>.cont .firmInfo .firmName{
	position: relative;
	padding-bottom: 10rpx;
	border-bottom:2rpx solid #F2F2F2;
}
.bottom .box .block>.cont .firmInfo .firmName .name{
	font-size: 32rpx;
	line-height: 42rpx;
	color: #333;
	width: 450rpx;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
}
.bottom .box .block>.cont .firmInfo .firmName .staus{
	position: absolute;
	right: -38rpx;
	top: 0;
	width: 86rpx;
	height: 40rpx;
	border-top-left-radius:20rpx;
	border-bottom-left-radius:20rpx;
	text-align: center;
	line-height: 40rpx;
	font-size: 24rpx;
	display: block;
	color: #fff;
}
.bottom .box .block>.cont .firmInfo .firmPro{
	color: #666;
	font-size: 24rpx;
	line-height: 32rpx;
}